sources:
    nginx_logs:
        type: file
        include: 
            - /var/log/nginx/my_access.log
        read_from: end

sinks:
    clickhouse:
        inputs: 
            - nginx_logs
        type: clickhouse
        endpoint: 127.0.0.1:8123
        database: nginxdb
        table: access_logs
        skip_unknown_fields: true
